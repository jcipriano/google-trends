<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title>Trends</title>

    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Lato:100,300,400,700" %>
    <%= stylesheet_link_tag "normalize" %>
    <%= stylesheet_link_tag "trends" %>

    <%= javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js" %>
    <%= javascript_include_tag "//cdnjs.cloudflare.com/ajax/libs/mustache.js/0.7.2/mustache.min.js" %>
    <%= javascript_include_tag "trends" %>
  </head>
  
  <body class="<%= page_classes %>">

    <%= yield %>

  </body>
</html>